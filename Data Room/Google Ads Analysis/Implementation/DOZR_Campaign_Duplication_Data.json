  "duplication_queries": {
    "get_ad_groups": "SELECT ad_group.id, ad_group.name, ad_group.status, ad_group.type FROM ad_group WHERE campaign.name = '[SOURCE_CAMPAIGN]' AND ad_group.status != 'REMOVED'",
    "get_ads": "SELECT ad_group.name, ad_group_ad.ad.id, ad_group_ad.ad.type, ad_group_ad.ad.final_urls, ad_group_ad.ad.responsive_search_ad.headlines, ad_group_ad.ad.responsive_search_ad.descriptions, ad_group_ad.ad.responsive_search_ad.path1, ad_group_ad.ad.responsive_search_ad.path2 FROM ad_group_ad WHERE campaign.name = '[SOURCE_CAMPAIGN]' AND ad_group_ad.status != 'REMOVED'",
    "get_keywords": "SELECT ad_group.name, ad_group_criterion.keyword.text, ad_group_criterion.keyword.match_type, ad_group_criterion.status, ad_group_criterion.final_url FROM keyword_view WHERE campaign.name = '[SOURCE_CAMPAIGN]' AND ad_group_criterion.status != 'REMOVED'",
    "get_negative_keywords_adgroup": "SELECT ad_group.name, ad_group_criterion.keyword.text, ad_group_criterion.keyword.match_type FROM ad_group_criterion WHERE campaign.name = '[SOURCE_CAMPAIGN]' AND ad_group_criterion.negative = true AND ad_group_criterion.type = 'KEYWORD'",
    "get_negative_keywords_campaign": "SELECT campaign_criterion.keyword.text, campaign_criterion.keyword.match_type FROM campaign_criterion WHERE campaign.name = '[SOURCE_CAMPAIGN]' AND campaign_criterion.type = 'KEYWORD' AND campaign_criterion.negative = true",
    "get_geo_exclusions": "SELECT campaign_criterion.location.geo_target_constant FROM campaign_criterion WHERE campaign.name = '[SOURCE_CAMPAIGN]' AND campaign_criterion.negative = true AND campaign_criterion.type = 'LOCATION'",
    "get_tracking": "SELECT campaign.tracking_url_template, campaign.final_url_suffix, campaign.url_custom_parameters FROM campaign WHERE campaign.name = '[SOURCE_CAMPAIGN]'"
  },

